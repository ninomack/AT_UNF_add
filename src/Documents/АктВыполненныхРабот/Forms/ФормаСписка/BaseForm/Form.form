  <attributes>
    <name>Список</name>
    <id>1000001</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Контрагент</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Ссылка</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА ДокументАктВыполненныхРабот.СуммаДокумента > 0
			ТОГДА ВЫБОР
					КОГДА НЕ ДокументАктВыполненныхРабот.Контрагент.ВестиРасчетыПоДокументам
							ИЛИ ФактОплаты.Сумма ЕСТЬ NULL
						ТОГДА -1
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) = 0
						ТОГДА 2
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) = ДокументАктВыполненныхРабот.СуммаДокумента
						ТОГДА 0
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) > ДокументАктВыполненныхРабот.СуммаДокумента
						ТОГДА 7
					ИНАЧЕ 1
				КОНЕЦ
		ИНАЧЕ -1
	КОНЕЦ КАК НомерКартинкиОплаты,
	ВЫБОР
		КОГДА ДокументАктВыполненныхРабот.СуммаДокумента > 0
			ТОГДА ВЫБОР
					КОГДА ФактОплаты.Сумма ЕСТЬ NULL
						ТОГДА &quot;Не учитывается&quot;
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) - ДокументАктВыполненныхРабот.СуммаДокумента >= 0
						ТОГДА &quot;Оплачен полностью&quot;
					КОГДА ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) > 0
						ТОГДА &quot;Оплачен частично&quot;
					ИНАЧЕ &quot;Без оплаты&quot;
				КОНЕЦ
		ИНАЧЕ &quot;Не учитывается&quot;
	КОНЕЦ КАК СтатусОплаты,
	ЕСТЬNULL(ФактОплаты.Сумма, 0) - ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) - ЕСТЬNULL(ФактОплаты.СуммаАванса, 0) КАК КОплате,
	ВЫБОР
		КОГДА ДокументАктВыполненныхРабот.Проведен
				И ДокументАктВыполненныхРабот.СуммаДокумента > 0
			ТОГДА ВЫРАЗИТЬ((ЕСТЬNULL(ФактОплаты.СуммаОплаты, 0) + ЕСТЬNULL(ФактОплаты.СуммаАванса, 0)) * 100 / ДокументАктВыполненныхРабот.СуммаДокумента КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК ПроцентОплаты,
	ДокументАктВыполненныхРабот.Ссылка КАК Ссылка,
	ДокументАктВыполненныхРабот.ПометкаУдаления КАК ПометкаУдаления,
	ДокументАктВыполненныхРабот.Номер КАК Номер,
	ДокументАктВыполненныхРабот.Дата КАК Дата,
	ДокументАктВыполненныхРабот.Проведен КАК Проведен,
	ДокументАктВыполненныхРабот.Организация КАК Организация,
	ДокументАктВыполненныхРабот.Контрагент КАК Контрагент,
	ДокументАктВыполненныхРабот.Договор КАК Договор,
	ДокументАктВыполненныхРабот.ДокументОснование КАК ДокументОснование,
	ДокументАктВыполненныхРабот.ЗаказПокупателя КАК ЗаказПокупателя,
	ДокументАктВыполненныхРабот.Подразделение КАК Подразделение,
	ДокументАктВыполненныхРабот.ВидЦен КАК ВидЦен,
	ДокументАктВыполненныхРабот.ВидСкидкиНаценки КАК ВидСкидкиНаценки,
	ДокументАктВыполненныхРабот.ВалютаДокумента КАК ВалютаДокумента,
	ДокументАктВыполненныхРабот.НалогообложениеНДС КАК НалогообложениеНДС,
	ДокументАктВыполненныхРабот.СуммаВключаетНДС КАК СуммаВключаетНДС,
	ДокументАктВыполненныхРабот.НДСВключатьВСтоимость КАК НДСВключатьВСтоимость,
	ДокументАктВыполненныхРабот.Курс КАК Курс,
	ДокументАктВыполненныхРабот.Кратность КАК Кратность,
	ДокументАктВыполненныхРабот.Ответственный КАК Ответственный,
	ДокументАктВыполненныхРабот.Комментарий КАК Комментарий,
	ДокументАктВыполненныхРабот.СуммаДокумента КАК СуммаДокумента,
	ДокументАктВыполненныхРабот.Автор КАК Автор,
	ДокументАктВыполненныхРабот.ПоложениеЗаказаПокупателя КАК ПоложениеЗаказаПокупателя,
	ДокументАктВыполненныхРабот.ДисконтнаяКарта КАК ДисконтнаяКарта,
	ДокументАктВыполненныхРабот.ПроцентСкидкиПоДисконтнойКарте КАК ПроцентСкидкиПоДисконтнойКарте,
	ДокументАктВыполненныхРабот.СкидкиРассчитаны КАК СкидкиРассчитаны,
	ДокументАктВыполненныхРабот.ХозяйственнаяОперация КАК ХозяйственнаяОперация,
	ДокументАктВыполненныхРабот.РаботыИУслуги.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Номенклатура КАК Номенклатура,
		Характеристика КАК Характеристика,
		Количество КАК Количество,
		ЕдиницаИзмерения КАК ЕдиницаИзмерения,
		Цена КАК Цена,
		ПроцентСкидкиНаценки КАК ПроцентСкидкиНаценки,
		СуммаСкидкиНаценки КАК СуммаСкидкиНаценки,
		Сумма КАК Сумма,
		СтавкаНДС КАК СтавкаНДС,
		СуммаНДС КАК СуммаНДС,
		Всего КАК Всего,
		ЗаказПокупателя КАК ЗаказПокупателя,
		Содержание КАК Содержание,
		ПроцентАвтоматическойСкидки КАК ПроцентАвтоматическойСкидки,
		СуммаАвтоматическойСкидки КАК СуммаАвтоматическойСкидки,
		КлючСвязи КАК КлючСвязи,
		НоменклатураНабора КАК НоменклатураНабора,
		ХарактеристикаНабора КАК ХарактеристикаНабора,
		ДоляСтоимости КАК ДоляСтоимости,
		ЗаказНаПроизводство КАК ЗаказНаПроизводство
	) КАК РаботыИУслуги,
	ДокументАктВыполненныхРабот.Предоплата.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Документ КАК Документ,
		НомерСертификата КАК НомерСертификата,
		Заказ КАК Заказ,
		СуммаРасчетов КАК СуммаРасчетов,
		Курс КАК Курс,
		Кратность КАК Кратность,
		СуммаПлатежа КАК СуммаПлатежа,
		ОплатаСертификатом КАК ОплатаСертификатом
	) КАК Предоплата,
	ДокументАктВыполненныхРабот.ДополнительныеРеквизиты.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Свойство КАК Свойство,
		Значение КАК Значение,
		ТекстоваяСтрока КАК ТекстоваяСтрока
	) КАК ДополнительныеРеквизиты,
	ДокументАктВыполненныхРабот.СкидкиНаценки.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		КлючСвязи КАК КлючСвязи,
		СкидкаНаценка КАК СкидкаНаценка,
		Сумма КАК Сумма
	) КАК СкидкиНаценки,
	ДокументАктВыполненныхРабот.МоментВремени КАК МоментВремени,
	СостоянияЭД.СостояниеВерсииЭД КАК СостояниеВерсииЭД,
	ВЫБОР
		КОГДА НаличиеФайлов.ЕстьФайлы ЕСТЬ NULL
			ТОГДА 0
		КОГДА НаличиеФайлов.ЕстьФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьФайлы
ИЗ
	Документ.АктВыполненныхРабот КАК ДокументАктВыполненныхРабот
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СостоянияЭД КАК СостоянияЭД
		ПО (СостоянияЭД.СсылкаНаОбъект = ДокументАктВыполненныхРабот.Ссылка)}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ФактОплатыДокументов КАК ФактОплаты
		ПО ДокументАктВыполненныхРабот.Ссылка = ФактОплаты.Документ}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК НаличиеФайлов
		ПО ДокументАктВыполненныхРабот.Ссылка = НаличиеФайлов.ОбъектСФайлами}</queryText>
      <mainTable>Document.АктВыполненныхРабот</mainTable>
